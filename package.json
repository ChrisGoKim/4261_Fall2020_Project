    "node-properties-parser": "0.0.2",
    "properties-parser": "^0.3.1",
    "vue": "^2.6.12",
    "vue-cordova": "^0.1.2",
    "vue-router": "^3.4.3",
    "vuetify": "^2.3.13"
  },
  "devDependencies": {
    "@mdi/font": "^5.6.55",
